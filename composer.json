{
  "name": "escolalms/reports",
  "description": "Escola Headless LMS Reports",
  "type": "package",
  "require": {
    "php": ">=7.4 | >=8.0",
    "escolalms/auth": ">=0.0",
    "escolalms/cart": ">=0.0",
    "escolalms/categories": ">=0.1",
    "escolalms/core": ">=0.1.6",
    "escolalms/courses": ">=0.0",
    "laravel/framework": ">=8.0",
    "laravel/socialite": "v5.1.3",
    "maatwebsite/excel": "^3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "orchestra/testbench": ">=5.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Paweł Chołaj",
      "email": "pawel.cholaj@escola.pl"
    }
  ],
  "autoload": {
    "psr-4": {
      "EscolaLms\\Reports\\": "src",
      "EscolaLms\\Reports\\Tests\\": "tests",
      "Database\\Factories\\EscolaLms\\Reports\\Models\\": "database/factories",
      "EscolaLms\\Reports\\Database\\Seeders\\": "database/seeders"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "EscolaLms\\Reports\\EscolaLmsReportsServiceProvider"
      ]
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@php ./vendor/bin/testbench package:discover --ansi"
    ]
  },
  "config": {
    "optimize-autoloader": false,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
